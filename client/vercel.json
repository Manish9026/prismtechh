{
    "version": 2,
    "builds": [
      { "src": "vite.config.js", "use": "@vercel/static-build" }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://prismtech-1.onrender.com/api/$1"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "https://prismtech-1.onrender.com/uploads/$1"
      }
    ]
  }